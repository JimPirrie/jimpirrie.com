{% extends "_page.html.twig" %}
{% block content %}
    <div class="row">
        <div class="col page-banner" style="height: 300px;background-image: url('/_images/carousel-waterfall.png');">
            <div class="page-banner-content">
                <h3><span class="mv-dynamic mv-item-type" data-ref="{{ mv_ref }}"></span></h3>
                <h1 class="pt-2"><span class="mv-dynamic mv-item-name" data-ref="{{ mv_ref }}"></span></h1>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-8 pt-3" style="white-space: nowrap; overflow: hidden; text-overflow: ellipsis;">
                <a href="/">home</a> <i class="fa-regular fa-arrow-right-long"></i> free <i class="fa-regular fa-arrow-right-long"></i> <strong><span class="mv-dynamic mv-item-name" data-ref="{{ mv_ref }}"></span> (<span class="mv-dynamic mv-item-type" data-ref="{{ mv_ref }}"></span>)</strong>
            </div>
            <div class="col-12 col-lg-4">
                <table class="float-end">
                    <tr>
                        <td>
                            <strong>Follow Jim:</strong>
                        </td>
                        <td>
                            <!-- Go to www.addthis.com/dashboard to customize your tools -->
                            <div class="addthis_inline_follow_toolbox d-inline-block"></div>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="col">
                    <h1 id="mv-free-title"></h1>
                    <div id="mv-free-content-1"></div>
                    <div id="mv-free-item"></div>
                    <div id="mv-free-content-2"></div>
                </div>
            </div>
        </div>
        <div style="height: 200px;"></div>
    </div>
{% endblock %}